{
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions":{
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "plugins":[
        "react"
    ],
    "rules":{
        "react/prop-types": 0
    },
    "env":{
        "browser": true,
        "node": true,
        "es6": true
    }

}